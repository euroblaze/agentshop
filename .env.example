# General LLM Settings
LLM_DEFAULT_PROVIDER=openai
LLM_CACHE_TTL=3600
LLM_ENABLE_CACHING=true
LLM_ENABLE_RATE_LIMITING=true
LLM_LOG_REQUESTS=true

# OpenAI Configuration
LLM_OPENAI_ENABLED=false
LLM_OPENAI_API_KEY=your_openai_api_key_here
LLM_OPENAI_ORGANIZATION=your_org_id_here
LLM_OPENAI_DEFAULT_MODEL=gpt-3.5-turbo
LLM_OPENAI_MAX_TOKENS=1000
LLM_OPENAI_TEMPERATURE=0.7
LLM_OPENAI_RATE_LIMIT_RPM=60
LLM_OPENAI_COST_LIMIT_DAILY=10.0

# Claude Configuration
LLM_CLAUDE_ENABLED=false
LLM_CLAUDE_API_KEY=your_claude_api_key_here
LLM_CLAUDE_DEFAULT_MODEL=claude-3-haiku-20240307
LLM_CLAUDE_MAX_TOKENS=1000
LLM_CLAUDE_TEMPERATURE=0.7
LLM_CLAUDE_RATE_LIMIT_RPM=60
LLM_CLAUDE_COST_LIMIT_DAILY=10.0

# Ollama Configuration
LLM_OLLAMA_ENABLED=false
LLM_OLLAMA_BASE_URL=http://localhost:11434
LLM_OLLAMA_DEFAULT_MODEL=llama2
LLM_OLLAMA_MAX_TOKENS=1000
LLM_OLLAMA_TEMPERATURE=0.7
LLM_OLLAMA_RATE_LIMIT_RPM=60
LLM_OLLAMA_COST_LIMIT_DAILY=0.0

# Perplexity Configuration
LLM_PERPLEXITY_ENABLED=false
LLM_PERPLEXITY_API_KEY=your_perplexity_api_key_here
LLM_PERPLEXITY_DEFAULT_MODEL=llama-3.1-sonar-small-128k-online
LLM_PERPLEXITY_MAX_TOKENS=1000
LLM_PERPLEXITY_TEMPERATURE=0.7
LLM_PERPLEXITY_RATE_LIMIT_RPM=60
LLM_PERPLEXITY_COST_LIMIT_DAILY=5.0

# Database Configuration
DATABASE_URL=sqlite:///agentshop.db

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=true
SECRET_KEY=your_secret_key_here

# Email Configuration (for notifications)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_email_password
SMTP_USE_TLS=true

# Payment Gateway Configuration
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_SECRET=your_paypal_secret

# Security Settings
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_HTTPONLY=true
SESSION_COOKIE_SAMESITE=Lax